        <clear />
        <add name="GravitateMembershipProvider" type="Gravitate.Security.Providers.GravitateMembershipProvider, Gravitate.Security" enablePasswordReset="true" maxInvalidPasswordAttempts="5" passwordAttemptWindow="10" />
