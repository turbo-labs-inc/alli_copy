  <!-- This section is an example of how to lock down a directory when using a Membership & RoleProvider 
    deny = * means deny everyone
    deny = ? means deny unauthenticated users-->
  <location path="App_Themes">
    <system.web>
      <authorization>
        <allow users="*" />
      </authorization>
    </system.web>
  </location>
  <location path="Images">
    <system.web>
      <authorization>
        <allow users="*" />
      </authorization>
    </system.web>
  </location>
